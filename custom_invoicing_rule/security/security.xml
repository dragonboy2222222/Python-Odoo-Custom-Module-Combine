  <record id="account_move_no_delete_rule" model="ir.rule">
    <field name="name">Restrict Delete for Custom Invoice Group</field>
    <field name="model_id" ref="account.model_account_move"/> <!-- Replace with your model -->
    <field name="domain_force">[('id', '!=', 0)]</field> <!-- Allow access to all records -->
    <field name="groups" eval="[(4, ref('group_invoice_custom_access'))]"/>
    <field name="perm_read" eval="1"/>
    <field name="perm_write" eval="1"/>
    <field name="perm_create" eval="1"/>
    <field name="perm_unlink" eval="0"/>
</record>




